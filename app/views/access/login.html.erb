<% @page_title= "Admin Login" %>

<div class="login">
      <%= form_tag(:action=> 'attempt_login') do %>
          <div class="container"><%= label_tag(:username) %><%= text_field_tag(:username) %></div>
          <div class="container"><%= label_tag(:password) %><%= password_field_tag(:password) %></div>
          <div class="button-container"><%= submit_tag("Log In") %></div>

</div>

   
<% end %>

